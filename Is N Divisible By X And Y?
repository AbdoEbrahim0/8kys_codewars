bool isDivisible(int n, int x, int y) {
  // your code here
//   bool ans;
//   ((n %x==0) && (n%y==0)) ? ans=true:ans=false;
//   return ans;
return !(n % x) && !(n % y);
}
int main() {
    
cout<<isDivisible(3,3,4)<<"\n"; //f
cout<<isDivisible(12,3,4)<<"\n";//t
cout<<isDivisible(48,3,4)<<"\n";//t
cout<<isDivisible(8,3,4)<<"\n";//f
cout<<isDivisible(100,5,3)<<"\n";//f
    return 0;

}

