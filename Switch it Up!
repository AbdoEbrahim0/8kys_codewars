std::string switch_it_up(int number){
map <int ,string >myMap ={{0,"Zero"},{1,"One"},{2,"Two"},{3,"Three"},{4,"Four"},{5,"Five"},{6,"six"},{7,"Seven"},{8,"Eight"},{9,"Nine"}};
if (number>=0 && number<10)
return (myMap [number]);
return "Only numbers range is 0-9 ";
}

