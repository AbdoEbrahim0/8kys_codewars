std::string countSheep(int number) {
  string s ;
  stringstream ss;
  //start from 1 =49 (hexa)
  for (int i=1;i<=number;i++)
  {
    ss<<i;
  s+= ss.str() + " sheep...";
  ss.str("");
  }
return s;
}
