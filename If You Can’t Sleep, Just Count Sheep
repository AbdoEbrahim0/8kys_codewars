//method 3:
  std::string countSheep(int number) {   
      string out = "";
      for (int k = 1; k <= number; k++)
      {
          int temp_n = k;
          int tenTimes = 0;
          while (temp_n >= 10)
          {
              temp_n = temp_n / 10;
              tenTimes++;
          }
          //cout << "tenTimes : " << tenTimes << "\n";
          //cout << "num is : " << k << endl;
          string str = "";
          int numOfDigits = tenTimes;
          int temp_k = k;
          for (int j = 0; j <= numOfDigits; j++)
          {
              int digit = floor(temp_k / pow(10, tenTimes));//302/100=3       // 2/100=0.02=0   // 2/100=
              str += char(digit + 48);
              temp_k = temp_k - (pow(10, tenTimes--)) * digit; //302-100*3=02     //2-100*0=2
          }
          out = out + str + " sheep...";
      }
      cout << out << endl;
  return out;
  }
int main() {
    //  cout<<int('1')<<"\n";
    //  cout<<char(49)<<"\n";
    //  cout<<int('2')<<"\n";
    //  cout<<char(50)<<"\n";
  cout<<countSheep(50)<<"\n";    

