// methode 3:
std::string fakeBin(std::string str){
  for (char &c:str)
  {
    if(c-'0'>=0 && c-'0'<=4)
    {
      c='0';
    }else if(c-'0'>=5 && c-'0'<10)
    {
      c='1';
    }
  }
  return  str;
}

int main() {
  
    // vector<int>vvv{ 1, 1, 11, 2, 3  };
    // vector<int>vvv {-6 ,-1 ,10 ,-12};
    string str="45385593107843568";
    cout<< fakeBin(str);    
   return 0;
}
