#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cassert>

#include <cstring>
using namespace std;

#include <string>

string even_or_odd(int number) 
{
  
  return (number & 1)? "odd":"even";
}
int main() {

cout<<even_or_odd(-1)<<"\n";     
cout<<even_or_odd(0)<<"\n"; 
cout<<even_or_odd(-21)<<"\n"; 
cout<<even_or_odd(-3)<<"\n"; 
cout<<even_or_odd(-12)<<"\n";

    return 0;

}

