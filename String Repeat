#include <string>
#include <algorithm>
#include <cstring>
using namespace std;
#include <vector>
#include <regex>
#include <string>
#include <numeric>
string repeat_str(size_t repeat, const string& str) {
  string temp="";
  while (repeat-- >0)
  {
    temp = temp+str ;
  }
  return temp;
}
int main() {
    string d="hello ";
   cout<<repeat_str(3,d )<<"\n";
    // cout<<string_to_number("-1000")<<"\n";
    // cout<<string_to_number("-321405")<<"\n";

return 0;
}

