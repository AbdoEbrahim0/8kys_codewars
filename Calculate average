#include <vector>
#include <regex>
#include <string>
#include <numeric>
double calcAverage(const std::vector<int>& values){
  double sum,s=values.size();

  for (int i=0;i<s;i++)
  {
    sum+=values.at(i);
  }
  return sum/s;
}
int main() {

vector<int>vec={2,5};
    cout<<calcAverage(vec)<<"\n";
  // string d="hello ";
  //   cout<<is_uppercase("HELLO WORLD")<<"\n";
