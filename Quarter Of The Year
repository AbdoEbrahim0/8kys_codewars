int quarterOf(int month)
{

// double d=;
// cout<<"d:"<<d<<endl;
    //method 1
    // return ceil((double)month/3);
    //method 2
    // return (month+2)/3;
    //method 3
    // int Q[] = {0,1,1,1,2,2,2,3,3,3,4,4,4};
    // return Q[month];
    //method 4
    return month>=10 ? 4 : month>=7 ? 3 : month>=4 ? 2 : 1;
}
